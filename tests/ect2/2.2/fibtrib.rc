function : int main(){
   int i = 1, j = 1;
   cout << 0 << " " << 1 << " ";
   while (j > 0){
      cout << j << " ";
      auto k = j;
      j = j + i;
      i = k;
   }
   cout << endl;
   
   int k = j = 1;
   i = 0;
   cout << 0 << " " << 1 << " ";
   while (k > 0){
      cout << k << " ";
      auto l = k;
      k = k + i + j;
      i = j;
      j = l;
   }
   cout << endl;

   return 0;
}
